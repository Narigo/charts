{
  "env": {
    "es6": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "overrides": [
    {
      "files": [
        "crawler/**/*.js"
      ],
      "env": {
        "es6": true,
        "node": true
      }
    },
    {
      "files": [
        "docs/**/*.spec.js"
      ],
      "env": {
        "jest": true
      }
    },
    {
      "files": [
        "docs/**/*.js"
      ],
      "env": {
        "browser": true
      }
    },
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "allowImportExportEverywhere": true,
    "ecmaVersion": 2019,
    "sourceType": "module"
  }
}